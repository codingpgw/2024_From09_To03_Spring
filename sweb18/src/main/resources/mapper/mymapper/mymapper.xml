<mapper namespace="com.pcwk.ehr.mymapper">

  <select id="doHello"
    parameterType="com.pcwk.ehr.mymapper.domain.MyMapperVO"
    resultType="com.pcwk.ehr.mymapper.domain.MyMapperVO"
  >
        SELECT #{userId}    ||' 금요일' as userId,
               #{password}  ||' 즐거운' as password 
          FROM dual
  </select>
   
  
   
